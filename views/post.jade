extends layout

block content
  form(method='POST' action= postUrl)
    label(for='nickname') Nickname:
    input#nickname.form-control(type='text', placeholder='user nickname' name='nickname')
    br
    label(for='uid') UID:
    input#uid.form-control(type='text', placeholder='UID' name='uid')
    br
    label(for='email') Email:
    input#email.form-control(type='text', placeholder='email' name='email')
    br
    label(for='password') Password:
    input#password.form-control(type='text', placeholder='password' name='password')
    br
    label(for='gender') gender:
    input#gender.form-control(type='text', placeholder='your gender' name='gender')
    br
    button.btn.btn-primary(type='submit') test